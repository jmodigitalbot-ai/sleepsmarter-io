{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "webhook-handler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/pdf/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/webhook-handler.js"
    },
    {
      "src": "/health",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}